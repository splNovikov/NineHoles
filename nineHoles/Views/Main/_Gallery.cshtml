<div class="gallery common-body">

	<ul ng-if="imgPaths && imgPaths.length > 0"
		rn-carousel
		rn-carousel-controls
		rn-carousel-auto-slide
		rn-carousel-transition="slide"
		rn-carousel-duration="500"
		class="image image-carusel-custom">

		<li ng-repeat="img in imgPaths">
			<span class="helper"></span>
			<div class="img-container">
				<img ng-src="{{img}}" />
			</div>
		</li>
	</ul>

</div>